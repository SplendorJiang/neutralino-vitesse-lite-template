{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/dist/vue/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*"
  ],
  "modes": {
    "window": {
      "title": "neutralino-vitesse-lite-template",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/dist/vue/icon.png"
    }
  },
  "cli": {
    "binaryName": "neutralino-vitesse-lite-template",
    "resourcesPath": "/dist/vue/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/public/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "exportAuthInfo": true,
    "frontendLibrary": {
      "patchFile": "/dist/vue/index.html",
      "devUrl": "http://localhost:3333"
    }
  }
}